{
    "$schema": "https://json.schemastore.org/eslintrc",
    "root": true,
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "@kayahr"
    ],
    "rules": {
        "@typescript-eslint/ban-types": [
            "error",
            {
                "extendDefaults": true,
                "types": {
                    "Function": false,
                    "object": false
                }
            }
        ]
    }
}
